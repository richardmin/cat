<div class="center jumbotron">
  <%= image_tag("ow_logo.png", :alt => "Overwatch Logo") %>
  <h1>Welcome to the Overwatch Matchmaker</h1>

  
  
  <% if user_signed_in? %>
    <%= link_to "Join Game!", join_game_path, class: "btn btn-lg btn-primary" %>
  <% else %>
    <%= link_to "Sign up now!", sign_up_path, class: "btn btn-lg btn-primary" %>
    <%= link_to "Join Game!", login_path, class: "btn btn-lg btn-primary" %>
  <% end %>
</div>